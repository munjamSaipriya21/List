a=[[2,0,4],[6,5,3],[9,8,7]]
i=0
c=[]
while i<len(a):
    j=0
    b=[]
    while j<len(a):
        b.append(a[j][i])
        j+=1
    i+=1
    c.append(b)
print(c)


